@model IEnumerable<CarBookProject.Models.ApplicationUser>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/AdminDashboard/Views/Shared/_AdminLayout.cshtml";
}





<p>
    <a asp-action="Create"class="btn btn-primary">Create New</a>
    <a asp-action="ActiveUser" class="btn btn-success">Active User </a>
</p>
<table class="table table-hover ">
    <thead >
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NormalizedUserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsDeleted)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsUpdated)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompanyAccount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GOLD)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SILVER)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PLATINUM)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NormalizedUserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsDeleted)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsUpdated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompanyAccount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GOLD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SILVER)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PLATINUM)
            </td>

            <td>
                <a class="btn btn-primary" asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a class="btn btn-info" asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
        }
    </tbody>
</table>

